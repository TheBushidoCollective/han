{
  "hooks": {
    "UserPromptSubmit": [
      {
        "hooks": [
          {
            "type": "command",
            "command": "han hook dispatch UserPromptSubmit",
            "timeout": 30
          }
        ]
      }
    ],
    "SessionStart": [
      {
        "hooks": [
          {
            "type": "command",
            "command": "han hook dispatch SessionStart",
            "timeout": 30
          }
        ]
      }
    ]
  },
  "enabledPlugins": {
    "bushido@han": true,
    "core@han": true,
    "disciplines/accessibility@han": true,
    "disciplines/plugin-development@han": true,
    "disciplines/content@han": true,
    "disciplines/frontend@han": true,
    "disciplines/prompts@han": true,
    "disciplines/documentation@han": true,
    "tools/blueprints@han": true,
    "services/github@han": true,
    "services/reddit@han": true,
    "tools/act@han": true,
    "patterns/ai-dlc@han": true,
    "patterns/atomic-design@han": true,
    "validation/biome@han": true,
    "tools/bun@han": true,
    "patterns/git-storytelling@han": true,
    "frameworks/gluestack@han": true,
    "frameworks/ink@han": true,
    "validation/markdown@han": true,
    "frameworks/nextjs@han": true,
    "tools/playwright@han": true,
    "tools/playwright-bdd@han": true,
    "frameworks/react@han": true,
    "frameworks/relay@han": true,
    "languages/rust@han": true,
    "validation/shellcheck@han": true,
    "languages/typescript@han": true
  },
  "extraKnownMarketplaces": {
    "han": {
      "source": {
        "source": "directory",
        "path": "./"
      }
    }
  }
}
