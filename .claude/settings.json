{
  "hooks": {
    "UserPromptSubmit": [
      {
        "hooks": [
          {
            "type": "command",
            "command": "han hook dispatch UserPromptSubmit",
            "timeout": 30000
          }
        ]
      }
    ],
    "SessionStart": [
      {
        "hooks": [
          {
            "type": "command",
            "command": "han hook dispatch SessionStart",
            "timeout": 30000
          }
        ]
      }
    ],
    "Stop": [
      {
        "hooks": [
          {
            "type": "command",
            "command": "${CLAUDE_PROJECT_DIR}/scripts/lint-plugins.sh"
          }
        ]
      }
    ]
  },
  "enabledPlugins": {
    "core@han": true,
    "bushido@han": true,
    "jutsu-act@han": true,
    "jutsu-biome@han": true,
    "jutsu-bun@han": true,
    "jutsu-ink@han": true,
    "jutsu-markdown@han": true,
    "jutsu-nextjs@han": true,
    "jutsu-playwright@han": true,
    "jutsu-react@han": true,
    "jutsu-typescript@han": true,
    "hashi-blueprints@han": true,
    "hashi-github@han": true,
    "hashi-playwright-mcp@han": true,
    "do-claude-plugin-development@han": true,
    "do-frontend-development@han": true,
    "do-technical-documentation@han": true,
    "do-prompt-engineering@han": true,
    "do-accessibility-engineering@han": true,
    "jutsu-shellcheck@han": true,
    "jutsu-git-storytelling@han": true,
    "do-content-creator@han": true
  },
  "extraKnownMarketplaces": {
    "han": {
      "source": {
        "source": "directory",
        "path": "./"
      }
    }
  }
}
