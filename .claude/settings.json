{
  "hooks": {
    "UserPromptSubmit": [
      {
        "hooks": [
          {
            "type": "command",
            "command": "han hook dispatch UserPromptSubmit",
            "timeout": 30
          }
        ]
      }
    ],
    "SessionStart": [
      {
        "hooks": [
          {
            "type": "command",
            "command": "han hook dispatch SessionStart",
            "timeout": 30
          }
        ]
      }
    ]
  },
  "enabledPlugins": {
    "bushido@han": true,
    "core@han": true,
    "accessibility@han": true,
    "plugin-development@han": true,
    "content@han": true,
    "frontend@han": true,
    "prompts@han": true,
    "documentation@han": true,
    "blueprints@han": true,
    "github@han": true,
    "reddit@han": true,
    "act@han": true,
    "ai-dlc@han": true,
    "atomic-design@han": true,
    "biome@han": true,
    "bun@han": true,
    "git-storytelling@han": true,
    "gluestack@han": true,
    "ink@han": true,
    "markdown@han": true,
    "nextjs@han": true,
    "playwright@han": true,
    "playwright-bdd@han": true,
    "react@han": true,
    "relay@han": true,
    "rust@han": true,
    "shellcheck@han": true,
    "typescript@han": true
  },
  "extraKnownMarketplaces": {
    "han": {
      "source": {
        "source": "directory",
        "path": "./"
      }
    }
  }
}
