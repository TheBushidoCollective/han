{
  "name": "relay",
  "version": "1.3.0",
  "description": "Advanced Relay GraphQL framework skills for React applications",
  "author": {
    "name": "The Bushido Collective",
    "url": "https://thebushido.co"
  },
  "homepage": "https://github.com/thebushidocollective/han",
