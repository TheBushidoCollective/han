{
  "name": "jutsu-rust",
  "version": "1.10.0",
  "description": "Advanced Rust skills for ownership, error handling, and async programming.",
  "author": {
    "name": "The Bushido Collective",
    "url": "https://thebushido.co"
  },
  "homepage": "https://github.com/thebushidocollective/han",
  "repository": "https://github.com/thebushidocollective/han",
  "license": "FSL-1.1-ALv2",
  "keywords": ["rust", "ownership", "async"],
  "lspServers": {
    "rust-analyzer": {
      "command": "${CLAUDE_PLUGIN_ROOT}/scripts/lsp-entrypoint.sh",
      "args": [],
      "extensionToLanguage": {
        ".rs": "rust"
      },
      "startupTimeout": 60000,
      "restartOnCrash": true,
      "maxRestarts": 3,
      "initializationOptions": {
        "files": {
          "excludeDirs": ["node_modules", ".git", "target", "vendor"]
        }
      }
    }
  }
}
