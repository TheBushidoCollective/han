{
  "hooks": {
    "Stop": [
      {
        "hooks": [
          {
            "type": "command",
            "command": "npx -y @thebushidocollective/han hook run jutsu-go format --fail-fast --cached",
            "timeout": 120
          },
          {
            "type": "command",
            "command": "npx -y @thebushidocollective/han hook run jutsu-go vet --fail-fast --cached",
            "timeout": 60
          },
          {
            "type": "command",
            "command": "npx -y @thebushidocollective/han hook run jutsu-go test --fail-fast --cached",
            "timeout": 900
          }
        ]
      }
    ]
  }
}
