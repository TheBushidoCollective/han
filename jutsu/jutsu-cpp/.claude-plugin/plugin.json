{
  "name": "jutsu-cpp",
  "version": "1.1.2",
  "description": "Advanced C++ programming skills for modern C++ and template metaprogramming",
  "author": {
    "name": "The Bushido Collective",
    "url": "https://thebushido.co"
  },
  "homepage": "https://github.com/thebushidocollective/han",
  "repository": "https://github.com/thebushidocollective/han",
  "license": "FSL-1.1-ALv2",
  "keywords": ["cpp", "c++", "modern-cpp", "templates"],
  "lspServers": {
    "clangd": {
      "command": "${CLAUDE_PLUGIN_ROOT}/scripts/lsp-entrypoint.sh",
      "args": ["--background-index"],
      "extensionToLanguage": {
        ".cpp": "cpp",
        ".hpp": "cpp",
        ".cc": "cpp",
        ".hh": "cpp",
        ".cxx": "cpp",
        ".hxx": "cpp",
        ".C": "cpp",
        ".H": "cpp"
      }
    }
  }
}
