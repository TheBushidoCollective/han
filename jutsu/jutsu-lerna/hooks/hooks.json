      {
        "hooks": [
          {
            "type": "command",
            "command": "npx -y @thebushidocollective/han hook run lerna-bootstrap --fail-fast --cache",
            "timeout": 300000
          }
        ]
      }
    ],
    "SubagentStop": [
      {
        "hooks": [
          {
            "type": "command",
            "command": "npx -y @thebushidocollective/han hook run lerna-bootstrap --fail-fast --cache",
            "timeout": 300000
          }
        ]
      }
    ]
  }
}
