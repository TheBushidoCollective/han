{
	"name": "han-website",
	"version": "1.29.2",
	"private": true,
	"scripts": {
		"dev": "bun run --bun next dev",
		"prebuild": "bun run scripts/generate-marketplace.ts && bun run scripts/generate-search-index.ts && bun run scripts/generate-rss.ts && bun run scripts/generate-sitemap.ts",
		"build": "bun run --bun next build",
		"start": "bun run --bun serve out -p 3000",
		"test": "bun test",
		"test:e2e": "bun run playwright test",
		"test:e2e:headed": "bun run playwright test --headed",
		"test:e2e:debug": "bun run playwright test --debug"
	},
	"dependencies": {
		"feed": "^4.2.2",
		"fuse.js": "7.1.0",
		"gray-matter": "4.0.3",
		"mermaid": "^11.12.2",
		"minimatch": "^10.1.1",
		"next": "^16.0.7",
		"next-mdx-remote": "^5.0.0",
		"next-themes": "^0.4.6",
		"react": "^19.2.1",
		"react-dom": "^19.2.1",
		"react-markdown": "10.1.0",
		"rehype-raw": "^7.0.0",
		"remark-gfm": "4.0.1"
	},
	"devDependencies": {
		"@biomejs/biome": "2.3.7",
		"@playwright/test": "1.56.1",
		"@tailwindcss/postcss": "^4.1.17",
		"@tailwindcss/typography": "0.5.19",
		"@types/bun": "latest",
		"@types/minimatch": "^6.0.0",
		"@types/node": "24.10.1",
		"@types/react": "19.2.6",
		"autoprefixer": "10.4.22",
		"postcss": "8.5.6",
		"serve": "^14.2.4",
		"tailwindcss": "4.1.17",
		"typescript": "5.9.3"
	}
}
