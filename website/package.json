{
	"name": "han-website",
	"version": "1.22.2",
	"private": true,
	"scripts": {
		"dev": "bun run --bun next dev",
		"prebuild": "bun run scripts/generate-marketplace.ts && bun run scripts/generate-search-index.ts",
		"build": "bun run --bun next build",
		"start": "bun run --bun serve out -p 3000",
		"test": "bun run playwright test",
		"test:headed": "bun run playwright test --headed",
		"test:debug": "bun run playwright test --debug",
		"test:unit": "bun run lib/search-utils.test.ts"
	},
	"dependencies": {
		"fuse.js": "7.1.0",
		"gray-matter": "4.0.3",
		"next": "^16.0.7",
		"react": "^19.2.1",
		"react-dom": "^19.2.1",
		"react-markdown": "10.1.0",
		"remark-gfm": "4.0.1"
	},
	"devDependencies": {
		"@biomejs/biome": "2.3.7",
		"@playwright/test": "1.56.1",
		"@tailwindcss/postcss": "^4.1.17",
		"@tailwindcss/typography": "0.5.19",
		"@types/bun": "latest",
		"@types/node": "24.10.1",
		"@types/react": "19.2.6",
		"autoprefixer": "10.4.22",
		"postcss": "8.5.6",
		"serve": "^14.2.4",
		"tailwindcss": "4.1.17",
		"typescript": "5.9.3"
	}
}
