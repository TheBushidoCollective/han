{
  "name": "@thebushidocollective/browse-client",
  "version": "0.1.0",
  "private": true,
  "description": "Han Dashboard - Browse Claude Code sessions, metrics, and settings",
  "type": "module",
  "scripts": {
    "dev": "relay-compiler --watch & bun run build/dev.ts",
    "build": "relay-compiler && bun run build/build.ts",
    "preview": "bun run build && cd out && bunx serve",
    "relay": "relay-compiler",
    "relay:watch": "relay-compiler --watch",
    "schema:pull": "cd ../han && bun run schema:export > ../browse-client/schema.graphql",
    "typecheck": "tsc --noEmit",
    "lint": "biome check .",
    "lint:fix": "biome check --write .",
    "bdd:generate": "bddgen",
    "test": "npm run bdd:generate && playwright test",
    "test:ui": "npm run bdd:generate && playwright test --ui",
    "test:headed": "npm run bdd:generate && playwright test --headed",
    "test:report": "playwright show-report"
  },
  "dependencies": {
    "anser": "^2.3.5",
    "graphql": "^16.9.0",
    "graphql-ws": "^5.16.0",
    "highlight.js": "^11.11.1",
    "marked": "^17.0.1",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-native-code-highlighter": "^1.3.0",
    "react-native-web": "^0.21.2",
    "react-relay": "^19.0.0",
    "react-router-dom": "^7.1.1",
    "react-syntax-highlighter": "^15.6.1",
    "relay-runtime": "^19.0.0"
  },
  "devDependencies": {
    "@biomejs/biome": "2.3.7",
    "@playwright/test": "^1.57.0",
    "@types/bun": "latest",
    "@types/node": "^22.0.0",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "@types/react-native": "^0.73.0",
    "@types/react-native-web": "0.19.2",
    "@types/react-relay": "^16.0.0",
    "@types/react-syntax-highlighter": "^15.5.13",
    "@types/relay-runtime": "^17.0.0",
    "playwright-bdd": "^8.4.2",
    "relay-compiler": "^19.0.0",
    "typescript": "5.9.3"
  }
}
