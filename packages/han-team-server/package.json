{
  "name": "@han/team-server",
  "version": "0.1.0",
  "description": "Han Team Platform - Multi-tenant Claude Code session analytics server",
  "type": "module",
  "main": "dist/server.js",
  "scripts": {
    "dev": "bun run --hot lib/server.ts",
    "build": "bun build lib/server.ts --outdir=dist --target=bun",
    "start": "bun dist/server.js",
    "migrate": "bun run lib/migrations/run.ts",
    "test": "bun test",
    "test:crypto": "bun test test/crypto.test.ts",
    "test:key-rotation": "bun test test/key-rotation*.test.ts",
    "test:watch": "bun test --watch",
    "typecheck": "tsc --noEmit",
    "schema:export": "bun run lib/graphql/export-schema.ts",
    "schema:print": "bun run lib/graphql/export-schema.ts > schema.graphql"
  },
  "dependencies": {
    "@envelop/depth-limit": "^7.1.0",
    "@noble/ciphers": "^2.1.1",
    "@noble/hashes": "^2.0.1",
    "@pothos/core": "^4.6.0",
    "@pothos/plugin-relay": "^4.5.0",
    "graphql": "^16.10.0",
    "graphql-yoga": "^5.10.10",
    "hono": "^4.6.15",
    "ioredis": "^5.4.2",
    "jose": "^5.9.6",
    "pg": "^8.13.1",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@types/bun": "^1.1.14",
    "@types/node": "^22.10.5",
    "@types/pg": "^8.11.10",
    "bun-types": "^1.3.8",
    "typescript": "^5.7.2"
  },
  "engines": {
    "node": ">=20"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/TheBushidoCollective/han.git",
    "directory": "packages/han-team-server"
  },
  "license": "MIT"
}
