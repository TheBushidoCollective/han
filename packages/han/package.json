{
	"name": "@thebushidocollective/han",
	"version": "2.3.1",
	"description": "CLI for installing and managing curated Claude Code plugins from the Han marketplace",
	"main": "dist/lib/main.js",
	"types": "dist/lib/main.d.ts",
	"type": "module",
	"bin": {
		"han": "bin/han.js"
	},
	"scripts": {
		"typecheck": "tsc --noEmit",
		"test": "bun test test/*.test.ts",
		"test:source": "HAN_TEST_SOURCE=true bun test test/*.test.ts",
		"lint": "biome check .",
		"lint:fix": "biome check --write .",
		"lint:config-sync": "bun scripts/lint-plugin-config-sync.ts",
		"convert-config": "bun scripts/convert-plugin-config.ts",
		"prepack": "npm test",
		"build:binary": "bun scripts/build-bundle.js",
		"build:bundle": "bun scripts/build-bundle.js",
		"schema:export": "bun lib/graphql/export-schema.ts"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/TheBushidoCollective/han.git"
	},
	"homepage": "https://han.guru",
	"bugs": {
		"url": "https://github.com/TheBushidoCollective/han/issues"
	},
	"keywords": [
		"bushido",
		"validation",
		"hooks",
		"claude",
		"claude-code",
		"testing",
		"ci",
		"plugins",
		"memory",
		"metrics"
	],
	"author": "The Bushido Collective",
	"license": "FSL-1.1-ALv2",
	"engines": {
		"node": ">=20"
	},
	"files": [
		"bin/",
		"schemas/",
		"README.md"
	],
	"optionalDependencies": {
		"@thebushidocollective/han-darwin-arm64": "1.31.5",
		"@thebushidocollective/han-darwin-x64": "1.31.5",
		"@thebushidocollective/han-linux-arm64": "1.31.5",
		"@thebushidocollective/han-linux-x64": "1.31.5",
		"@thebushidocollective/han-win32-x64": "1.31.5"
	},
	"devDependencies": {
		"@anthropic-ai/claude-agent-sdk": "0.1.50",
		"@biomejs/biome": "2.3.7",
		"@opentelemetry/api": "^1.9.0",
		"@opentelemetry/exporter-logs-otlp-proto": "^0.208.0",
		"@opentelemetry/exporter-metrics-otlp-proto": "^0.208.0",
		"@opentelemetry/resources": "^2.2.0",
		"@opentelemetry/sdk-logs": "^0.208.0",
		"@opentelemetry/sdk-metrics": "^2.2.0",
		"@opentelemetry/sdk-node": "^0.208.0",
		"@opentelemetry/semantic-conventions": "^1.38.0",
		"@pothos/core": "^4.5.0",
		"@pothos/plugin-relay": "^4.4.0",
		"@types/marked-terminal": "6.1.1",
		"@types/micromatch": "^4.0.10",
		"@types/node": "24.10.1",
		"@types/react": "19.2.6",
		"@types/react-dom": "^19.0.0",
		"@types/relay-runtime": "^17.0.0",
		"@types/ws": "^8.18.1",
		"babel-plugin-relay": "^19.0.0",
		"bun-types": "^1.3.4",
		"commander": "14.0.2",
		"dotenv": "^17.2.3",
		"esbuild": "^0.27.0",
		"graphql": "^16.10.0",
		"graphql-relay": "^0.10.2",
		"graphql-ws": "^5.16.0",
		"graphql-yoga": "^5.10.11",
		"gray-matter": "^4.0.3",
		"ink": "6.5.1",
		"ink-select-input": "^6.2.0",
		"ink-spinner": "5.0.0",
		"ink-testing-library": "^4.0.0",
		"ink-text-input": "^6.0.0",
		"marked-terminal": "7.3.0",
		"next": "^15.0.0",
		"react": "19.2.3",
		"react-devtools-core": "^6.1.5",
		"react-dom": "^19.0.0",
		"react-relay": "^19.0.0",
		"relay-runtime": "^19.0.0",
		"typescript": "5.9.3",
		"vite": "^6.3.5",
		"ws": "^8.18.3",
		"yaml": "^2.8.1"
	},
	"dependencies": {
		"@graphql-yoga/plugin-defer-stream": "^3.18.0",
		"@pothos/plugin-directives": "^4.2.5",
		"@tanstack/react-virtual": "^3.13.13",
		"dataloader": "^2.2.3",
		"highlight.js": "^11.11.1",
		"marked": "^17.0.1",
