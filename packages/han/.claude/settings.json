{
  "hooks": {
    "UserPromptSubmit": [
      {
        "hooks": [
          {
            "type": "command",
            "command": "han hook dispatch UserPromptSubmit",
            "timeout": 30000
          }
        ]
      }
    ],
    "SessionStart": [
      {
        "hooks": [
          {
            "type": "command",
            "command": "han hook dispatch SessionStart",
            "timeout": 30000
          }
        ]
      }
    ]
  },
  "enabledPlugins": {
    "core@han": true,
    "bushido@han": true,
    "jutsu-typescript@han": true,
    "hashi-github@han": true
  }
}
