{
  "name": "@thebushidocollective/han-native",
  "version": "0.1.0",
  "description": "High-performance native bindings for Han CLI hook operations",
  "main": "index.js",
  "types": "index.d.ts",
  "napi": {
    "name": "han-native",
    "triples": {
      "defaults": true,
      "additional": [
        "aarch64-apple-darwin",
        "x86_64-apple-darwin",
        "x86_64-unknown-linux-gnu",
        "aarch64-unknown-linux-gnu",
        "x86_64-pc-windows-msvc"
      ]
    }
  },
  "files": [
    "index.js",
    "index.d.ts",
    "*.node"
  ],
  "scripts": {
    "artifacts": "napi artifacts",
    "build": "napi build --platform --release && npm run copy-to-han",
    "copy-to-han": "mkdir -p ../han/native && cp *.node ../han/native/han-native.node && (xattr -c ../han/native/han-native.node 2>/dev/null || true)",
    "build:debug": "napi build --platform",
    "prepublishOnly": "napi prepublish -t npm",
    "universal": "napi universal",
    "version": "napi version"
  },
  "devDependencies": {
    "@napi-rs/cli": "^2.18.4"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/thebushidocollective/han"
  },
  "license": "FSL-1.1-ALv2",
  "author": "The Bushido Collective",
  "keywords": [
    "napi-rs",
    "native",
    "rust",
    "performance",
    "glob",
    "sha256"
  ],
  "engines": {
    "node": ">= 18"
  }
}
